<?xml version="1.0"?>
<settings>
  <!-- Template only. filter_wg_channels.py will overwrite filename -->
  <filename>output/guide.xml</filename>

  <!-- new + nocache -->
  <mode>n#</mode>

  <!-- siteini.pack is copied into .wg++ at runtime -->
  <siteini>siteini.pack</siteini>

  <logging>on</logging>

  <retry time-out="5" skip="y">4</retry>

  <timespan days="2"/>

  <!-- force update -->
  <update>f</update>
  
  <!-- User agent to help with internet detection and site compatibility -->
  <user-agent>Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36</user-agent>
</settings>
